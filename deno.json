{
  "tasks": {
    "cache": "deno cache src/main.ts",
    "check": "deno check src/main.ts",
    "run": "deno run -A --unstable-cron src/main.ts",
    "update": "deno run -A https://deno.land/x/udd/main.ts **/*.ts"
  },
  "imports": {
    "@std/": "https://deno.land/std@0.222.1/",
    "oak": "https://deno.land/x/oak@14.2.0/mod.ts",
    "cors": "https://deno.land/x/cors@v1.2.2/mod.ts",
    "ptera": "https://deno.land/x/ptera@v1.0.2/mod.ts",
    "twitter-api-v2": "npm:twitter-api-v2@1.14.3"
  },
  "lock": false,
  "fmt": {
    "include": ["src/"]
  },
  "lint": {
    "include": ["src/"]
  }
}
